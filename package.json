{
  "name": "timestamp-nano",
  "description": "Timestamp for 64-bit time_t, nanosecond precision and strftime",
  "version": "0.99.714",
  "author": "@kawanet",
  "bugs": {
    "url": "https://github.com/kawanet/timestamp-nano/issues"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.4",
    "@types/node": "^10.5.2",
    "coveralls": "^3.0.2",
    "int64-buffer": "^0.1.10",
    "jshint": "^2.9.5",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "strftime": "^0.10.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2",
    "uglify-js": "^3.4.4",
    "zuul": "^3.12.0"
  },
  "homepage": "https://github.com/kawanet/timestamp-nano#readme",
  "jshintConifg": {
    "undef": true,
    "unused": true,
    "node": true
  },
  "keywords": [
    "date",
    "datetime",
    "microsecond",
    "nanosecond",
    "strftime",
    "time_t",
    "timestamp"
  ],
  "license": "MIT",
  "main": "timestamp.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/timestamp-nano.git"
  },
  "scripts": {
    "build": "make",
    "coveralls": "./node_modules/.bin/nyc npm run mocha && ./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "fixpack": "fixpack",
    "jshint": "./node_modules/.bin/jshint .",
    "mocha": "./node_modules/.bin/mocha test/*.js",
    "test": "make test",
    "tsc": "./node_modules/.bin/tsc",
    "tsc-watch": "./node_modules/.bin/tsc --watch",
    "zuul-local": "./node_modules/.bin/zuul --local 4000 --ui mocha-bdd test/*.js"
  },
  "typings": "typings/timestamp.d.ts"
}
